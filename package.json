  "type": "commonjs",
  "bugs": {
    "url": "https://github.com/d-arsya/coba-express-api/issues"
  },
  "homepage": "https://github.com/d-arsya/coba-express-api#readme",
  "description": "",
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@types/express": "^5.0.0",
    "eslint": "^9.17.0",
    "globals": "^15.14.0",
    "husky": "^9.1.7",
    "prettier": "3.4.2",
    "pretty-quick": "^4.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.18.1"
  },
  "dependencies": {
    "express": "^4.21.2"
  }
